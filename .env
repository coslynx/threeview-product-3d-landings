# Environment variables for 3D Landing Page

# Application title
VITE_APP_TITLE="3D Landing Page Template"

# Base URL for 3D models (GLTF files)
VITE_MODEL_PATH="/models/"

# Base URL for textures (PNG, JPG, KTX2)
VITE_TEXTURE_PATH="/textures/"

# Base URL for Draco decoder files
VITE_DRACO_DECODER_PATH="/draco/"

# Base URL for environment map (HDR or equirectangular image)
VITE_ENVIRONMENT_MAP_PATH="/environment/"

# Enable performance monitor (true or false)
VITE_PERFORMANCE_MONITOR="false"

# Base URL for the application API
VITE_API_BASE_URL="/api/"